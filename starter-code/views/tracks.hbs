<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Listen</th>
        </tr>
    </thead>
    <tbody>
        {{#each tracks}}
        <tr class="body">
            <td>{{name}}</td>
            <td>
                <audio controls src="{{preview_url}}"></audio>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>